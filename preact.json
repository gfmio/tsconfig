{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./app.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "types": ["preact"],
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "paths": {
      "react": ["./node_modules/preact/compat/"],
      "react-dom": ["./node_modules/preact/compat/"]
    }
  }
}