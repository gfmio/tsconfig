{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "types": ["astro/client"],
    "jsx": "preserve",
    "jsxImportSource": "astro",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "allowJs": true,
    "isolatedModules": true,
    "resolveJsonModule": true
  }
}
