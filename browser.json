{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "lib": ["ESNext", "DOM", "DOM.Iterable", "WebWorker.ImportScripts"],
    "types": [],
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "allowJs": true,
    "isolatedModules": true
  }
}