{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "incremental": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "target": "ES2022",
    "types": [
      "react",
      "react-dom",
      "node",
      "next"
    ]
  },
  "extends": "./react.json"
}
