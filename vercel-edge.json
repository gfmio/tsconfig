{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "lib": ["ESNext"],
    "types": ["@vercel/edge"],
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "allowJs": true,
    "isolatedModules": true,
    "resolveJsonModule": true
  }
}
